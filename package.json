{
  "name": "now-postgraphile",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "classnames": "^2.2.6",
    "graphiql": "^0.17.5",
    "graphiql-explorer": "^0.5.1",
    "graphql": "14.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/prettier": "^1.19.1",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.0",
    "@types/testing-library__jest-dom": "^5.0.2",
    "@types/testing-library__react": "^10.0.0",
    "husky": "^4.2.3",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "prettier-plugin-import-sort": "^0.0.4",
    "react-scripts": "3.4.1",
    "typescript": "~3.8.3",
    "typesync": "^0.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --no-color --loglevel warn --write '{src,scripts,db,types}/**/*.{js,jsx,ts,tsx,css,scss,json,graphql}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "jsxSingleQuote": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "prettier --no-color --write",
      "yarn lint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
